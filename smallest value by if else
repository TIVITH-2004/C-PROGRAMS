#include<stdio.h>
void main()
{
    int a,b;
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        printf("b is smallest");
    }
    else
    {
         printf("a is smallest");
    }
}
//
#include<stdio.h>
void main()
{
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if(a<b)
    {
        printf("a is smallest");
    }
    else if(b<c)
    {
        printf("b is smallest ");
    }
    else
    {
        printf(" c is smallest");
    }
}
