#include<stdio.h>
void main()
{
    int a,b,ch;
    scanf("%d%d",&a,&b);
    printf("1.addition\n2.subtraction\n3.multiplication\n4.division\n",ch);
    scanf("%d",&ch);
    switch(ch)
    {
        case 1:
              printf("addition: %d",a+b);
              break;
        case 2:
              printf("subtraction: %d",a-b);
              break;
        case 3:
              printf("multiplication: %d",a*b);
               break;
        case 4:
              printf("division: %d",a/b);
              break;
        default:
              printf("invalid");
    }

}

//


#include<stdio.h>
#include<math.h>
void main()
{
    double a,b,ch;
    scanf("%lf%lf",&a,&b);
    printf("1.addition\n2.subtraction\n3.multiplication\n4.division\n5.modulud\n6.average\n7.power");
    scanf("%lf",&ch);
    switch(ch)
    {
        case 1:
              printf("addition: %lf",a+b);
              break;
        case 2:
              printf("subtraction: %lf",a-b);
              break;
        case 3:
              printf("multiplication: %lf",a*b);
               break;
        case 4:
              printf("division: %lf",a/b);
              break;
        case 5:
              printf("modulus: %alignof",a%b);
              break;
        case 6:
              printf("average: %lf",a+b/2);
              break;
        case 7:
              printf("power: %lf,pow(a,b));
              break;
        default:
              printf("invalid");
    }

}
