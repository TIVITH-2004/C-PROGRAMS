#include <stdio.h>

void main()
{
    char c[10],b,d[100];
    float f;
    printf("enter name: \n");
    scanf("%s",c);
   // printf("%s\n",c);
    printf("enter marks: \n");
    scanf("%f",&f);
   // printf("%f\n",f);
    printf("enter grade: \n");
    scanf(" %c",&b);
   // printf("%s\n",b);
    printf("enter location: \n");
    scanf( "%s",d);
   // printf("%s",d);
    printf("your name: %s\n",c);
   printf("your marks: %f\n",f);
    printf("your grade: %c\n",b);
    printf("your location: %s\n",d);

}
