#include<stdio.h>
void main()
{
    int n,m,sum=0,i;
    printf("enter the starting value: ");
    scanf("%d",&n);
    printf("enter the end value: ");
    scanf("%d",&m);
    for(i=n;i<=m;i++)
    {
        if(i%9==0)
        {
            printf("%d\n",i);
            sum=sum+i;
        }
    }
    printf("sum of the the digits:%d",sum);
}
