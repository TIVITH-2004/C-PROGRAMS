#include<stdio.h>
void main()
{
    int n=10,i;
    for(i=1;i<=10;i++)
    {
        printf("%d\n",i);
    }
}
#include<stdio.h>
void main()
{
    int n,i;
    printf("enter the value of n:");
    scanf("%d",&n);
    for(i=1;i<=10;i++){
        printf("%d\n",i);
    }

}
#include<stdio.h>
void main()
{
    int n,i=10;
    //printf("enter the value of n:");
    //scanf("%d",&n);
    do{
        printf("%d\n",i);
        i--;
    }
    while(i>=0);
    }
#include<stdio.h>
void main()
{
    int n,i=1;
    printf("enter the vakue of n: ");
    scanf("%d",&n);
    while(i<=10)
    {
        printf("%d+%d=%d\n",i,n,i+n);
        i++;
    }

}
